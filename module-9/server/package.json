{
  "name": "metana-express-rest-api",
  "version": "1.0.0",
  "description": "Demo of a REST API written in Node with Express.js and MongoDB as database backend",
  "main": "index.js",
  "scripts": {
    "start:backend": "nodemon ./index.js",
    "start:frontend": "cd ../client && npm start",
    "build:frontend": "cd ../client && npm run build",
    "prod": "NODE_ENV=production node index.js",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed": "node scripts/seedDb.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fieldse/metana-fullstack-demos.git"
  },
  "keywords": [
    "rest",
    "demo",
    "api",
    "rest-api",
    "express",
    "mongo"
  ],
  "author": "nathan obenchain -- compadre2049@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Compadre2049"
  },
  "homepage": "https://github.com/Compadre2049",
  "dependencies": {
    "all": "^0.0.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.7.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.3",
    "validator": "^13.12.0"
  },
